{
  "$id": "healthcare_job",
  "$schema": "https://schema.yext.com/config/km/field-presentation/v1",
  "entityType": "job",
  "displayName": "Job",
  "presentationEntries": [
    {
      "displayName": "Content",
      "entryType": "DIVIDER"
    },
    {
      "displayName": "Core Information",
      "entryType": "SECTION",
      "fields": [
        {
          "id": "name",
          "visibleOnAdd": true
        },
        {
          "id": "location"
        },
        {
          "id": "hiringOrganization"
        },
        {
          "id": "entityId",
          "visibleOnAdd": true
        },
        {
          "id": "healthcare_primaryCTA"
        }
      ]
    },
    {
      "displayName": "Job Details",
      "entryType": "SECTION",
      "fields": [
        {
          "id": "description"
        },
        {
          "id": "datePosted"
        },
        {
          "id": "validThrough"
        },
        {
          "id": "employmentType"
        },
        {
          "id": "workRemote"
        }
      ]
    },
    {
      "displayName": "Media",
      "entryType": "SECTION",
      "fields": [
        {
          "id": "logo"
        }
      ]
    },
    {
      "defaultCollapsed": true,
      "displayName": "Additional Attributes",
      "entryType": "SECTION",
      "fields": [
        {
          "id": "keywords"
        }
      ]
    },
    {
      "defaultCollapsed": true,
      "displayName": "Landing Pages",
      "entryType": "SECTION",
      "fields": [
        {
          "id": "applicationUrl"
        },
        {
          "id": "landingPageUrl"
        }
      ]
    },
    {
      "displayName": "Other",
      "entryType": "DIVIDER"
    },
    {
      "defaultCollapsed": true,
      "displayName": "Internal Use Only",
      "entryType": "SECTION",
      "fields": [
        {
          "id": "folder"
        },
        {
          "id": "labels"
        },
        {
          "id": "timezone"
        },
        {
          "id": "slug"
        }
      ]
    },
    {
      "displayName": "Settings",
      "entryType": "DIVIDER"
    },
    {
      "defaultCollapsed": true,
      "displayName": "Knowledge Assistant",
      "entryType": "SECTION",
      "fields": [
        {
          "id": "primaryConversationContact"
        },
        {
          "id": "nudgeEnabled"
        }
      ]
    }
  ]
}